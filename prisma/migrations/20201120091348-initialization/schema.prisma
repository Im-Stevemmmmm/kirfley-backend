datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

generator typegraphql {
  provider = "typegraphql-prisma"
  output   = "../src/generated/typegraphql-prisma"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  email    String @unique
  password String
}
